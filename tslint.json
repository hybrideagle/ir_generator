{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "interface-name": [false],
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    }
}
